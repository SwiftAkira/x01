{
  "name": "speedlink-backend",
  "version": "1.0.0",
  "description": "SpeedLink MVP Backend - REST API and Real-Time Services",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:rest": "tsx watch src/rest-api/server.ts",
    "dev:realtime": "tsx watch src/realtime/server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:rest": "node dist/rest-api/server.js",
    "start:realtime": "node dist/realtime/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "node dist/database/migrate.js",
    "db:seed": "node dist/database/seed.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "speedlink",
    "real-time",
    "websocket",
    "fastify",
    "socket.io",
    "motorcycle",
    "gps"
  ],
  "author": "SpeedLink Development Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^7.0.3",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.5",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "redis": "^4.6.13",
    "socket.io": "^4.7.4",
    "socket.io-redis": "^6.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vercel/node": "^5.5.4",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
